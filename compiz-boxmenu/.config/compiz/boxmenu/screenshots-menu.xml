	<item type="launcher">
		<icon>desktop</icon>
		<name>Whole Desktop (5 second delay)</name>
		<command>censored_screenshot.sh 5 yes</command>
	</item>
	<item type="launcher">
		<icon>desktop</icon>
		<name>Whole Desktop (10 second delay)</name>
		<command>censored_screenshot.sh 10 yes</command>
	</item>
	<item type="launcher">
		<icon>desktop</icon>
