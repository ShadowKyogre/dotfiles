	</item>
	<item type="launcher">
		<icon>windowlist2</icon>
		<name>Choose Window</name>
		<command>sh -c 'sleep 5 &amp;&amp; maim -sl -c 0,0.5,1,0.6 --opengl --magnify "/tmp/$(date +%F-%H%M%S).png"'</command>
	</item>
	<item type="launcher">
		<command>sh -c 'sleep 5 &amp;&amp; maim -i $(xdotool getactivewindow) "/tmp/$(date +%F-%H%M%S).png"'</command>
		<name>Current Window</name>
	</item>
	<item type="launcher">
		<name>Start Streaming</name>
		<icon>media-record</icon>
		<command>ytstream.sh</command>
	</item>
	<item type="launcher">
		<icon>media-playback-stop</icon>
		<name>Stop Streaming</name>
		<command>pkill -f ytstream.sh</command>
	</item>
</menu>
