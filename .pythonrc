def sample_multi(entries, min=0, max=None):
	if max is None:
		max = len(entries)
	this_many = random.randint(min,max)
	return random.sample(entries, this_many)

def mat_list(string):
	el_separator = re.compile(r',|, | ')
	result = []
	for sublist in string.strip().split(';'):
		print(el_separator.split(sublist))
		result.append([ float(s) for s in el_separator.split(sublist) ])
	return result